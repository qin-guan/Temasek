{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.Extensions.Hosting": "Warning",
      "Temasek.CalendarSyncWorker": "Debug"
    }
  },
  "CalendarSync": {
    "ServiceAccountJsonCredential": "BASE64_ENCODED_SERVICE_ACCOUNT_JSON",
    "PrimaryCalendarId": "your-primary-calendar@gmail.com",
    "SecondaryCalendarId": "your-secondary-calendar@gmail.com",
    "SyncIntervalMs": 60000,
    "MaxRetryAttempts": 3,
    "InitialRetryDelayMs": 5000,
    "MaxBatchSize": 100,
    "PerformFullSyncOnStartup": true
  }
}
